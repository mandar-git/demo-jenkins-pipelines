library identifier: 'mylibraryname@master',
    // 'mylibraryname' is just an identifier, it can be anything you like
    // 'master' refers to a valid git ref (branch)
    retriever: modernSCM([
      $class: 'GitSCMSource',
      remote: 'https://github.com/mandar-git/shared-libs.git'
])

pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                hello()
            }
        }
         stage('Greet') {
            steps {
                greet 'new user'
            }
        }
    }
}
